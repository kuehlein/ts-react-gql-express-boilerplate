  },
  "scripts": {
    "postinstall": "touch secrets.js",
    "watch-server": "NODE_ENV=development HMR=disabled npx tsc-watch --onSuccess \"nodemon ts-sourcemap/server/index.js\" --onFailure \"echo \u001b[31;1mERROR: Compilation Failed ---\u001b[33;1m Fix errors to restart server.\u001b[0m\"",
    "hot": "tsc && NODE_ENV=development HMR=enabled node ts-sourcemap/server/index.js",
    "prod": "tsc && NODE_ENV=production npx webpack --config=ts-sourcemap/webpack.prod.config.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "license": "ISC",
