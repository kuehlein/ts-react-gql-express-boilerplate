{
  "name": "ts-react-gql-express-boilerplate",
  "version": "0.0.1",
  "description": "Starter code for a Typescript App using GraphQL.",
  "keywords": [
    "boilerplate",
    "typescript",
    "graphql",
    "react",
    "redux",
    "postgresql",
    "express"
  ],
  "author": "Kyle Uehlein",
  "main": "index.ts",
  "repository": {
    "type": "git",
    "url": "git://github.com/kuehlein/ts-react-gql-express-boilerplate.git"
  },
  "scripts": {
    "postinstall": "touch secrets.js",
    "type-check": "npm run compile-prod --noEmit",
    "compile-dev": "tsc -p config/tsconfig.server.json",
    "compile-prod": "tsc -p config/tsconfig.prod.json",
    "server-dev": "NODE_ENV=development DATABASE_URL=postgres://localhost/ts-react-gql-express-boilerplate",
    "start-server": "npm run server-dev && node public/dist/ts-sourcemap/server/index.js",
    "watch-server": "npm run server-dev && nodemon public/dist/ts-sourcemap/server/index.js",
    "dev": "npm run compile-dev && npm run start-server",
    "prod": "npm run compile-prod && NODE_ENV=production npx webpack --config=public/dist/ts-sourcemap/webpack.prod.config.js",
    "meh": "tsc -p config/tsconfig.client.json",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "license": "ISC",
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-proposal-decorators": "^7.1.2",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.1.0",
    "@types/clean-webpack-plugin": "^0.1.2",
    "@types/compression": "0.0.36",
    "@types/express": "^4.16.0",
    "@types/html-webpack-plugin": "^3.2.0",
    "@types/morgan": "^1.7.35",
    "@types/react": "^16.4.18",
    "@types/react-dom": "^16.0.9",
    "@types/react-redux": "^6.0.9",
    "@types/webpack": "^4.4.17",
    "@types/webpack-dev-middleware": "^2.0.2",
    "@types/webpack-hot-middleware": "^2.16.4",
    "clean-webpack-plugin": "^0.1.19",
    "fork-ts-checker-webpack-plugin": "^0.4.12",
    "html-webpack-plugin": "^3.2.0",
    "ts-loader": "^5.2.2",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.15.0",
    "webpack-cli": "^3.1.2",
    "webpack-dev-middleware": "^3.4.0",
    "webpack-hot-middleware": "^2.24.3",
    "webpack-uglify-js-plugin": "^1.1.9"
  },
  "dependencies": {
    "@babel/core": "^7.1.2",
    "@babel/polyfill": "^7.0.0",
    "babel-runtime": "^6.26.0",
    "body-parser": "^1.18.3",
    "chalk": "^2.4.1",
    "compression": "^1.7.3",
    "express": "^4.16.4",
    "morgan": "^1.9.1",
    "pg": "^7.5.0",
    "pg-hstore": "^2.3.2",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-hot-loader": "^4.3.11",
    "react-redux": "^5.0.7",
    "reflect-metadata": "^0.1.12",
    "sequelize": "^4.39.1",
    "sequelize-typescript": "^0.6.6",
    "typescript": "^3.1.3",
    "webpack": "^4.22.0"
  }
}
